{
	"name": "base-template-front",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"serve": "vite",
		"build-dev": "vue-tsc --noEmit && vite build --mode dev",
		"build-stg": "vue-tsc --noEmit && vite build --mode stg",
		"build-prod": "vue-tsc --noEmit && vite build",
		"lint": "prettier --write '**/*.{ts,vue}' && eslint --fix",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown",
		"*.ts": "eslint --fix"
	},
	"dependencies": {
		"@fullcalendar/core": "^6.0.2",
		"@fullcalendar/daygrid": "^6.0.2",
		"@fullcalendar/interaction": "^6.0.2",
		"@fullcalendar/moment": "^6.0.2",
		"@fullcalendar/timegrid": "^6.0.2",
		"@fullcalendar/vue3": "^6.0.2",
		"@quasar/extras": "^1.15.0",
		"@vueuse/core": "^8.4.2",
		"@vueuse/integrations": "^7.7.1",
		"axios": "^0.27.2",
		"lodash-es": "^4.17.21",
		"moment": "^2.29.4",
		"pinia": "^2.0.17",
		"quasar": "^2.7.5",
		"vue": "^3.2.37",
		"vue-jwt-decode": "^0.1.0",
		"vue-moment": "^4.1.0",
		"vue-router": "^4.1.2",
		"vue3-json-excel": "^1.0.10-alpha",
		"vue3-smooth-dnd": "^0.0.2"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "^1.1.1",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^18.6.1",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"@vitejs/plugin-vue": "^3.0.0",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^9.3.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"sass": "1.32.12",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.3",
		"vue-tsc": "^0.38.4"
	}
}
